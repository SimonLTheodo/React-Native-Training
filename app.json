{
  "expo": {
    "name": "RN-Training",
    "slug": "RN-Training",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.simonltheodo.RNTraining"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.simonltheodo.RNTraining"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "bccbdf60-b1be-4dc9-92a1-5ccdae6512c9"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/bccbdf60-b1be-4dc9-92a1-5ccdae6512c9"
    },
    "owner": "simonl-rn-training",
    "plugins": [
      [
        "expo-router",
        {
          "asyncRoutes": {
            "default": "development",
            "android": false
          },
          "origin": "http://localhost:19000"
        }
      ]
    ],
    "scheme": "simonl-rn-training"
  }
}
